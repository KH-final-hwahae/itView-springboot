<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="itView.springboot.mapper.UserMapper">
<select id="findByUsername" parameterType="string" resultType="itView.springboot.vo.User">
select * from "USER" where user_id = #{userId}
</select>

<select id="selectUserByKakaoId" parameterType="string" resultType="itView.springboot.vo.User">
SELECT * FROM "USER" WHERE KAKAO_ID = #{kakaoId}
</select>

</mapper>